{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Google's stock price over time.",
  "title": "Life expextancy (1960 - 2020)",
  "data": {
    "url": "https://raw.githubusercontent.com/BrianMwangi74/FIT3179_A2/master/data/time_data.json"
  },
  "height": 200,
  "width": 500,
  "mark": "area",
  "encoding": {
    "x": {"field": "Year", "type": "quantitative", "axis": {"tickCount": 5}},
    "y": {
      "field": "Life expectancy",
      "type": "quantitative",
      "scale": {"type": "linear", "domain": [40, 80]}
    },
    "tooltip": [
      {"field": "Year", "type": "quantitative"},
      {"field": "Life expectancy", "type": "quantitative"}
    ]
  },
  "config": {}
}